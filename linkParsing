parsename

******************************* MANGAHERE ************************************************
wget -q -U 'Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.8.1.6) Gecko/20070802 SeaMonkey/1.1.4' -O- http://www.mangahere.com/manga/the_gamer | grep the_gamer | head -5 | tail -1 | grep -oE c[0-9]\{1,4\} | sed 's/^c0\?//g'

cat Links | grep $mangaName | awk -F ' ' '{print $3}' | sed 's/.*\///g'
or
http://www.mangahere.com/manga/the_gamer | sed 's/.*\///g'

******************************* MANGAPANDA or MANGAREADER*********************************
wget -O- -q http://www.mangapanda.com/the-gamer | grep the-gamer | head -2 | tail -1 | grep -oE [0-9]\{1,4\} | head -1
echo http://www.mangapanda.com/the-gamer | sed 's/.*\///g'
******************************* MANGACOW *************************************************
wget -O- -q http://mangacow.co/The-Gamer | grep The-Gamer | head -5 | tail -1 | grep -oE [0-9]\{,4\} | head -1

sed 's/.*\///g'
