******************************* MANGATOWN **************************************************
wget -O- -q http://www.mangatown.com/manga/the_gamer | grep the_gamer | head -5 | tail -1 | grep -oE c[0-9]\{1,4\} | sed 's/^c0\?//g

sed 's/.*\///g'
******************************* KISSMANGA **************************************************

wget -q -O- http://kissmanga.com/Manga/Bleach | grep Bleach | head | tail -1 | grep -oE [0-9]\{1,4\} | head -1
sed 's/.*\///g'




